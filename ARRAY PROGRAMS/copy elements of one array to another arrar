#include <stdio.h>
int main()
{
    int arr[5],arr2[5];
    printf("Please enter 5 numbers:\n");
    for(int i=1; i <= 5; i++){
        scanf("%d", &arr[i-1]);
    }
    for(int i=0; i <5; i++){
        arr2[i]=arr[i];
    }
    printf("elements of arr\n");
    for(int i=0; i <5; i++){
        printf("%d\n", arr[i]);
    }
    printf("elements of arr2");
    for(int i=0; i <5; i++){
        printf("%d\n", arr2[i]);
    }
        
    return 0;
}
