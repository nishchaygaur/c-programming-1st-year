#include <stdio.h>
int main()
{
    int max,min,a[5];
    printf("Please enter 5 numbers:\n");
    for(int i=1; i <= 5; i++){
        scanf("%d", &a[i-1]);
    }
    max=a[0];
    min=a[0];
    for(int i=0; i <5; i++){
        if (a[i]>max){
            max=a[i];
        }
        else if (a[i]<min){
            min = a[i];
        }
    }
     printf("max = %d\nmin = %d", max,min);
    return 0;
}
